{
  "name": "react-mock-component",
  "version": "2.3.1",
  "description": "Create type safe mock React components to use in tests",
  "keywords": [
    "tdd",
    "react",
    "spy",
    "stub",
    "mock",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NiGhTTraX/react-mock-component.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/react-mock-component",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "tdd-buffet test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "dependencies": {
    "@types/react": "~16.9.2",
    "@types/sinon": "~9.0.0",
    "sinon": "~9.0.0"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~6.1.0",
    "@tdd-buffet/react": "~1.1.0",
    "@tdd-buffet/tsconfig": "~1.0.0",
    "@tdd-buffet/jest-config": "~1.0.2",
    "@types/node": "~12.12.0",
    "@types/react-dom": "~16.9.0",
    "codecov": "~3.7.0",
    "eslint": "~7.0.0",
    "husky": "~4.2.0",
    "lint-staged": "~10.2.0",
    "react": "~16.13.0",
    "react-dom": "~16.13.0",
    "tdd-buffet": "~1.0.0",
    "typescript": "~3.9.0"
  }
}
