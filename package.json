{
  "name": "react-mock-component",
  "version": "2.1.0",
  "description": "Create type safe mock React components to use in tests",
  "keywords": [
    "tdd",
    "react",
    "spy",
    "stub",
    "mock",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NiGhTTraX/react-mock-component.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/react-mock-component",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "tdd-buffet test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "dependencies": {
    "@types/react": "~16.9.2",
    "@types/sinon": "~7.5.0",
    "sinon": "~9.0.0"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~4.1.1-alpha.0",
    "@tdd-buffet/react": "~0.8.0",
    "@types/node": "~12.12.0",
    "@types/react-dom": "~16.9.0",
    "@typescript-eslint/eslint-plugin": "~2.21.0",
    "codecov": "~3.6.0",
    "eslint": "~6.8.0",
    "eslint-import-resolver-typescript": "~2.0.0",
    "eslint-plugin-import": "~2.20.0",
    "eslint-plugin-prettier": "~3.1.2",
    "eslint-plugin-react": "~7.18.0",
    "eslint-plugin-react-hooks": "~2.5.0",
    "husky": "~4.2.0",
    "lint-staged": "~10.0.0",
    "prettier": "~1.19.1",
    "react": "~16.13.0",
    "react-dom": "~16.13.0",
    "tdd-buffet": "~0.15.0",
    "typescript": "~3.8.0"
  }
}
